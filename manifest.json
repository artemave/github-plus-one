{
  "name": "GitHub +1",
  "manifest_version": 2,
  "version": "0.6",
  "description": "+1 GitHub issues/pull requests",
  "icons": {
    "16": "github_g16.png",
    "48": "github_g48.png",
    "128": "github_g128.png"
  },
  "content_scripts": [{
    "matches": ["https://github.com/*"],
    "js": ["jquery.js", "contentscript.js"]
  }],
  "web_accessible_resources": ["rainbow.gif"]
}
